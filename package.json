  },
  "devDependencies": {
    "@types/react": "^16.9.26",
    "@graphql-codegen/typescript-react-apollo": "^1.12.1",
    "@graphql-codegen/typescript-operations": "^1.12.1",
    "eslint-config-prettier": "^6.5.0",
    "eslint-plugin-prettier": "^3.1.1",
    "husky": "^3.0.9",
    "jest": "^24.9.0",
    "prettier": "^1.18.2",
    "react": "^16.13.1",
    "react-apollo": "^3.1.0",
    "ts-jest": "^24.0.2",
    "typescript": "^3.6.3"
